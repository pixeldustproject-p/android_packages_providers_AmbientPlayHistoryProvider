<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pixeldust.ambient.play.history"
    android:versionCode="1"
    android:versionName="1.0">

    <permission
        android:name="com.pixeldust.ambient.play.history.MANAGE_HISTORY"
        android:protectionLevel="signature|privileged" />

    <uses-sdk
        android:minSdkVersion="28"
        android:targetSdkVersion="28" />

    <application
        android:allowBackup="false"
        android:allowClearUserData="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name">
        <provider
            android:name=".AmbientPlayHistoryProvider"
            android:authorities="com.pixeldust.ambient.play.history.provider"
            android:exported="true"
            android:readPermission="com.pixeldust.ambient.play.history.MANAGE_HISTORY" />
    </application>

</manifest>
